{
	"name": "rave-curl-text",
	"version": "0.2.0",
	"description": "Adds curl's text AMD plugin to RaveJS.",
	"main": "rave.js",
	"dependencies": {
		"curl": "~0.8",
		"rave-amd-plugins": "~0.2"
	},
	"rave": {
		"extension": "rave",
		"overrides": {
			"curl": {
				"directories": {
					"lib": "bower_components/curl/src/curl"
				}
			}
		}
	},
	"moduleType": [
		"node"
	],
	"keywords": [
		"rave",
		"rave-extension",
		"cujo",
		"cujojs",
		"amd",
		"plugin"
	],
	"license": "MIT",
	"homepage": "https://github.com/unscriptable/rave-curl-text",
	"authors": [
		"John Hann (http://unscriptable.com)",
		"Brian Cavalier (http://hovercraftstudios.com)"
	],
	"ignore": [
		"**/.*",
		"node_modules",
		"bower_components",
		"test",
		"tests"
	]
}
