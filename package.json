{
	"name": "rave-curl-text",
	"version": "0.2.0",
	"description": "Adds curl's text AMD plugin to RaveJS.",
	"main": "rave.js",
	"rave": {
		"extension": "rave",
		"overrides": {
			"curl": {
				"location": "node_modules/curl/src/curl"
			}
		}
	},
	"peerDependencies": {
		"curl": "~0.8",
		"rave-amd-plugins": "~0.2"
	},
	"repository": {
		"type": "git",
		"url": "git@github.com:unscriptable/rave-curl-text.git"
	},
	"keywords": [
		"rave",
		"rave-extension",
		"cujo",
		"cujojs",
		"amd",
		"plugin"
	],
	"license": "MIT",
	"homepage": "https://github.com/unscriptable/rave-curl-text",
	"contributors": [
		"John Hann (http://unscriptable.com)",
		"Brian Cavalier (http://hovercraftstudios.com)"
	]
}
